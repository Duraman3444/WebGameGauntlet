{
  "name": "web-game-gauntlet",
  "version": "1.0.0",
  "description": "3D Web Game with Three.js and Socket.io Multiplayer",
  "main": "src/server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server/server.js",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node src/server/server.js"
  },
  "keywords": [
    "threejs",
    "socketio",
    "multiplayer",
    "3d",
    "web-game"
  ],
  "author": "WebGameGauntlet",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "three": "^0.155.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "vite": "^4.4.9"
  }
}
