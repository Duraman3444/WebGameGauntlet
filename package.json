{
  "name": "threejs-multiplayer-game",
  "version": "1.0.0",
  "description": "A real-time multiplayer 3D game built with Three.js and Socket.io",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js"
  },
  "keywords": ["threejs", "game", "multiplayer", "3d", "socket.io"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "vite": "^5.0.0"
  }
} 